<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Hello!</title>
    </head>
    <body>
        <h1>Here are some delicious foods!</h1>
@{    
    List<string> StringList = new List<string>() {"Apple Pie", "Pizza", "Clinnamon Rolls", "Lasagna", "Donuts", "Chocolate Cake", "Burritos"};            
    foreach(string word in StringList)    
    {        
        <p><h3>@word</p>
    }
}
    </body>
</html>